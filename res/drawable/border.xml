	</item>
	<item>
	    <shape android:shape="rectangle" >
	        <padding
	            android:bottom="12dp"
	            android:left="12dp"
	            android:right="12dp"
	            android:top="12dp" />
	
	        <solid android:color="#ffffffff" />
	    </shape>
	</item>
</layer-list>